{
	"name": "backend-base",
	"version": "1.0.1",
	"description": "",
	"main": "dist/index",
	"types": "dist/index",
	"scripts": {
		"start": "node --preserve-symlinks -r ts-node/register/transpile-only ./src/index.ts",
		"debug": "node --preserve-symlinks --inspect-brk -r ts-node/register/transpile-only ./src/index.ts",
		"test": "ts-node ./src/tests/tests.ts",
		"test-debug": "node --inspect-brk -r ts-node/register/transpile-only ./src/tests/tests.ts",
		"test-with-coverage": "nyc --extension .ts npm test",
		"lint": "eslint src/**/*.ts",
		"prepublishOnly": "rm -rf dist && tsc -p tsconfig.src.json"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/cevek/back-base"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"bin": {
		"base": "dist/cli.js"
	},
	"files": [
		"dist",
		"template",
		"tsconfig.base.json",
		"*.js"
	],
	"dependencies": {
		"@types/bunyan": ">=1.8.6",
		"@types/cors": ">2.8.3",
		"@types/dotenv": ">=6.1.0",
		"@types/express": ">=4.16.1",
		"@types/express-graphql": ">0.6.0",
		"@types/express-session": ">=1.15.12",
		"@types/fs-extra": ">5.0.0",
		"@types/graphql": ">=14.0.7",
		"@types/node": ">=11.11.3",
		"@types/parcel-bundler": "^1.10.2",
		"@types/pg": ">7.4.0",
		"@types/request": ">2.48.0",
		"@typescript-eslint/eslint-plugin": ">1.4.0",
		"@typescript-eslint/eslint-plugin-tslint": ">1.4.0",
		"bunyan": ">1.8.0",
		"cors": ">2.8.0",
		"dataloader": ">=1.4.0",
		"dotenv": ">=7.0.0",
		"eslint": ">=5.15.2",
		"express": ">4.16.0",
		"express-graphql": ">0.7.0",
		"express-session": ">1.15.0",
		"fs-extra": ">7.0.0",
		"graphql": ">14.1.0",
		"nyc": ">=13.3.0",
		"pg": ">=7.9.0",
		"pm2": ">=3.4.0",
		"request": ">2.80.0",
		"ts-node": ">=8.0.3",
